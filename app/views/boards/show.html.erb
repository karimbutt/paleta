<div class="row">
    <div class="col-sm-3 one-more">

  <div id="board-show-title">
    <span>COLOR PROFILE<span>
  </div>
  <div id="board-show-subtitle">
    <span>Hover and click on values to build your custom palette</span>
  </div>
  </div>
    <div class="col-sm-9" style="padding-right: 50px; padding-left:100px; padding-top: 29px">
      <div class="shades"></div><div class="tints"></div>
      <div class="clearfix"></div>
    </div>
  </div>


<div class="row">

  <div class="col-sm-7">
    <div class="panel panel-default">
      <div class="mini-chart">
        <div class="col-sm-6">
          <div class="mini-set-bar-chart"></div>
        </div>
      </div>

      <div class="bar-chart"></div>

      <span class="instructions brackets">Click the brackets for a closer look at your most prevalent colors</span>

      <div class="toggle-buttons" style="margin-top:9px">


        <div class="set-selection imgBox" id="first" style="margin-right:15px"></div>
        <div class="set-selection imgBox" id="second" style="margin-right:15px"></div>
        <div class="set-selection imgBox" id="third" style="margin-right:15px"></div>
        <div class="set-selection imgBox" id="fourth" style="margin-right:15px"></div>
        <div class="set-selection imgBox" id="fifth" style=""></div>
      </div>

      <div class="clearfix"></div>
      <!-- <div class="panel-heading">a. Most prevalent colors</div> -->
    </div>
  </div>

  <div class="col-sm-4 col-sm-offset-2 palette-styling">
    <div class="panel panel-default">
    <div class="subtitle" id="suggested">RECOMMENDED PALETTES</div>
    <% @colour_lovers_palette.each do |palette| %>
      <div class="suggested-palette">
        <% palette.each do |color| %>
          <div class="suggested-color" data-color="#<%= color %>" style="width: 50px; height: 50px; background: #<%= color %>; float: left; "></div>
        <% end %>
      </div>
    <% end %>
    </div>

    <div>
      <div id="floating-palette" class="col-sm-2">
        <div id="selected-colors-div">
          <div class="subtitle" id="custom">CUSTOM PALETTE</div>
          <div class="selected-colors"></div>
        </div>
      </div>
    </div>
  </div>

</div>

<div class="row">

  <div class="col-sm-2">
  <!-- <div class="panel panel-default pie-charts-content"> -->
    <div class="subtitle aggregate">CMYK</div>
    <!-- <center><div class="cmyk-pie-chart"></div></center> -->
    <div class="cmyk-pie-chart"></div>
  </div>

  <div class="col-sm-2">
      <div class="subtitle aggregate aggregate-one">RGB</div>
      <!-- <center><div class="rgb-pie-chart"></div></center> -->
      <div class="rgb-pie-chart"></div>
    <!-- </div> -->
  </div>

   <!-- <div class="col-sm-10"></div> -->

  <div class="col-sm-1 col-move">
    <div class="subtitle complementary baseing one-more">BASE</div>
    <div id="chosen-color">
    </div>
  </div>

  <div class="col-sm-1 col-move">
    <div class="subtitle complementary baseing one-more">COMPLEMENTARY</div>
    <div id="complementary-color"></div>
<!--     <div class="panel-heading">Complementary</div>
